{
  "version": "1.0.0-*",
  "description": "Entity Framework Core .NET Command Line Tools. Includes dotnet-ef.",
  "buildOptions": {
    "emitEntryPoint": true,
    "outputName": "dotnet-ef",
    "keyFile": "../../tools/Key.snk",
    "compile": {
      "includeFiles": [
        "../ef/Extensions/CommandLineApplicationExtensions.cs",
        "../ef/External/CommandArgument.cs",
        "../ef/External/CommandLineApplication.cs",
        "../ef/External/CommandOption.cs",
        "../ef/External/CommandOptionType.cs",
        "../ef/External/CommandParsingException.cs",
        "../ef/Utilities/AnsiConsole.cs",
        "../ef/Utilities/AnsiConstants.cs",
        "../ef/Utilities/AnsiTextWriter.cs"
      ]
    },
    "embed": {
      "includeFiles": "Resources/EntityFrameworkCore.targets"
    }
  },
  "packOptions": {
    "tags": [
      "Entity Framework Core",
      "entity-framework-core",
      "EF",
      "Data",
      "O/RM"
    ],
    "packageType": "DotnetCliTool"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.1",
          "type": "platform"
        }
      }
    }
  }
}