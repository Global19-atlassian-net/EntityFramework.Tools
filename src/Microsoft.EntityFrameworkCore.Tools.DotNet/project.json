{
  "version": "1.0.0-msbuild1-final",
  "description": ".NET Core SDK command line tools for Entity Framework Core.",
  "buildOptions": {
    "emitEntryPoint": true,
    "outputName": "dotnet-ef",
    "keyFile": "../../tools/Key.snk",
    "compile": {
      "include": [
        "../ef/Extensions/CommandLineApplicationExtensions.cs",
        "../Shared/**/*.cs"
      ]
    }
  },
  "packOptions": {
    "tags": [
      "Entity Framework Core",
      "entity-framework-core",
      "EF",
      "Data",
      "O/RM"
    ],
    "packageType": "DotnetCliTool"
  },
  "dependencies": {
    "Microsoft.DotNet.Cli.Utils": "1.0.0-preview3-004050",
    "Microsoft.DotNet.ProjectModel": "1.0.0-rc3-003121"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.1",
          "type": "platform"
        }
      }
    }
  }
}