{
  "version": "1.0.0-*",
  "description": "Entity Framework Core .NET Command Line Tools. Includes dotnet-ef.",
  "buildOptions": {
    "emitEntryPoint": true,
    "outputName": "dotnet-ef",
    "keyFile": "../../tools/Key.snk",
    "compile": {
      "includeFiles": [
        "../ef/CommandLineUtils/CommandArgument.cs",
        "../ef/CommandLineUtils/CommandLineApplication.cs",
        "../ef/CommandLineUtils/CommandLineApplicationExtensions.cs",
        "../ef/CommandLineUtils/CommandOption.cs",
        "../ef/CommandLineUtils/CommandOptionType.cs",
        "../ef/CommandLineUtils/CommandParsingException.cs",
        "../ef/Commands/CommandBase.cs",
        "../ef/Commands/ContextCommandBase.cs",
        "../ef/Commands/DatabaseCommand.cs",
        "../ef/Commands/DbContextCommand.cs",
        "../ef/Commands/EFCommandBase.cs",
        "../ef/Commands/EnvironmentCommandBase.cs",
        "../ef/Commands/HelpCommandBase.cs",
        "../ef/Commands/MigrationsCommand.cs",
        "../ef/Commands/RootCommand.cs",
        "../ef/AnsiConsole.cs",
        "../ef/AnsiConstants.cs",
        "../ef/AnsiTextWriter.cs",
        "../ef/CommandException.cs",
        "../ef/Json.cs",
        "../ef/Reporter.cs"
      ],
      "include": [
        "../ef/Commands/**/*.Configure.cs"
      ]
    },
    "embed": {
      "includeFiles": "Resources/EntityFrameworkCore.targets"
    }
  },
  "packOptions": {
    "tags": [
      "Entity Framework Core",
      "entity-framework-core",
      "EF",
      "Data",
      "O/RM"
    ],
    "packageType": "DotnetCliTool"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.0",
          "type": "platform"
        }
      }
    }
  }
}